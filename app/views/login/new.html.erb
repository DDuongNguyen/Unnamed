<h1 class="sub-header">
  LOG IN PAGE!!!!!
</h1>

<% if flash[:messages] %>
  <ul class= "messages">
    <%= flash[:messages].to_s %>
  </ul>
    <% end %>


<div class="login">
<%= form_tag '/login/new' do %>
  <%= label_tag :user_name %>
  <br>
  <%= text_field_tag :user_name %>
  <br>
  <%= label_tag :password %>
  <br>
  <%= password_field_tag :password %>
  <br>
  <br>
  <%= submit_tag "SIGN ME IN", class: 'btn btn-small'%>
<% end %>
</div>
<br>
